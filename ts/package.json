{
  "name": "skel-ts",
  "version": "1.0.0",
  "description": "Skeleton project generation for typescript",
  "main": "create-ts-app.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && (chmod ug+x build/create*)",
